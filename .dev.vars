# Development environment variables for Cloudflare Workers
# These are automatically loaded when running `wrangler dev`

# Backend API configuration
API_LENS_BACKEND_URL="http://localhost:8000"
API_LENS_BACKEND_TOKEN="development-backend-token"

# Encryption for sensitive data
ENCRYPTION_KEY="dev-encryption-key-32-chars-long"

# Webhook secret for validation
WEBHOOK_SECRET="dev-webhook-secret-key"

# OpenAI configuration
OPENAI_API_URL="https://api.openai.com"
OPENAI_DEFAULT_MODEL="gpt-3.5-turbo"

# Anthropic configuration
ANTHROPIC_API_URL="https://api.anthropic.com"
ANTHROPIC_DEFAULT_MODEL="claude-3-haiku-20240307"

# Google AI configuration
GOOGLE_AI_API_URL="https://generativelanguage.googleapis.com"
GOOGLE_AI_DEFAULT_MODEL="gemini-1.0-pro"

# Development settings
ENVIRONMENT="development"
LOG_LEVEL="debug"
DEBUG_MODE="true"